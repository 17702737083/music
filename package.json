{
 "name": "ant-design-pro",
 "version": "5.0.0-beta.3",
 "private": true,
 "description": "An out-of-box UI solution for enterprise applications",
 "scripts": {
  "analyze": "cross-env ANALYZE=1 umi build",
  "build": "cross-env REACT_APP_ENV=dev umi build",
  "build:dev": "cross-env REACT_APP_ENV=dev umi build",
  "build:qas": "cross-env REACT_APP_ENV=qas umi build",
  "build:prd": "cross-env REACT_APP_ENV=prd umi build",
  "deploy": "yarn build:prd && docker build . -t harbor.wks.wistron.com.cn/k8swksoa/prd/web:latest && docker push harbor.wks.wistron.com.cn/k8swksoa/prd/web:latest",
  "dev": "npm run start:dev",
  "gh-pages": "gh-pages -d dist",
  "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
  "postinstall": "umi g tmp",
  "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
  "lint-staged": "node -v",
  "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
  "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
  "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
  "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
  "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
  "openapi": "umi openapi",
  "precommit": "node -v",
  "prettier": "prettier -c --write \"src/**/*\"",
  "start": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
  "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
  "start:dev:core": "cross-env REACT_APP_ENV=core-debug MOCK=none umi dev",
  "start:qas": "cross-env REACT_APP_ENV=qas MOCK=none umi dev",
  "start:prd": "cross-env REACT_APP_ENV=prd MOCK=none umi dev",
  "start:no-mock": "cross-env MOCK=none umi dev",
  "start:no-ui": "cross-env UMI_UI=none umi dev",
  "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
  "pretest": "node ./tests/beforeTest",
  "test": "umi test",
  "test:all": "node ./tests/run-tests.js",
  "test:component": "umi test ./src/components",
  "tsc": "tsc --noEmit"
 },
 "lint-staged": {
  "**/*.less": "stylelint --syntax less",
  "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
  "**/*.{js,jsx,tsx,ts,less,md,json}": [
   "prettier --write"
  ]
 },
 "browserslist": [
  "> 1%",
  "last 2 versions",
  "not ie <= 10"
 ],
 "dependencies": {
  "@ant-design/charts": "^1.1.20",
  "@ant-design/icons": "^4.6.2",
  "@ant-design/pro-card": "^1.14.5",
  "@ant-design/pro-descriptions": "^1.8.7",
  "@ant-design/pro-form": "^1.31.2",
  "@ant-design/pro-layout": "^6.21.1",
  "@ant-design/pro-list": "^1.10.6",
  "@ant-design/pro-table": "^2.43.4",
  "@umijs/route-utils": "^1.0.37",
  "antd": "^4.16.6",
  "bizcharts": "^4.1.11",
  "classnames": "^2.3.1",
  "install": "^0.13.0",
  "lodash": "^4.17.21",
  "moment": "^2.29.1",
  "omit.js": "^2.0.2",
  "rc-tween-one": "^2.7.3",
  "react": "^17.0.2",
  "react-csv": "^2.0.3",
  "react-dev-inspector": "^1.6.0",
  "react-dnd": "^14.0.2",
  "react-dom": "^17.0.2",
  "react-helmet-async": "^1.0.9",
  "react-text-loop": "^2.3.0",
  "reqwest": "^2.0.5",
  "umi": "^3.5.4",
  "xlsx": "^0.17.0"
 },
 "devDependencies": {
  "@ant-design/pro-cli": "^2.1.1",
  "@types/classnames": "^2.3.1",
  "@types/express": "^4.17.13",
  "@types/history": "^4.7.9",
  "@types/jest": "^26.0.24",
  "@types/lodash": "^4.14.171",
  "@types/react": "^17.0.13",
  "@types/react-dom": "^17.0.8",
  "@types/react-helmet": "^6.1.1",
  "@umijs/fabric": "^2.6.2",
  "@umijs/openapi": "^1.1.19",
  "@umijs/plugin-blocks": "^2.2.2",
  "@umijs/plugin-esbuild": "^1.3.1",
  "@umijs/plugin-openapi": "^1.3.0",
  "@umijs/preset-ant-design-pro": "^1.3.2",
  "@umijs/preset-dumi": "^1.1.23",
  "@umijs/preset-react": "^1.8.14",
  "@umijs/yorkie": "^2.0.5",
  "carlo": "^0.9.46",
  "cross-env": "^7.0.3",
  "cross-port-killer": "^1.3.0",
  "detect-installer": "^1.0.2",
  "enzyme": "^3.11.0",
  "eslint": "^7.30.0",
  "express": "^4.17.1",
  "gh-pages": "^3.2.3",
  "jsdom-global": "^3.0.2",
  "lint-staged": "^11.0.0",
  "mockjs": "^1.1.0",
  "prettier": "^2.3.2",
  "puppeteer-core": "^10.1.0",
  "stylelint": "^13.13.1",
  "typescript": "^4.3.5"
 },
 "engines": {
  "node": ">=10.0.0"
 },
 "gitHooks": {
  "commit-msg": "node -v"
 }
}
